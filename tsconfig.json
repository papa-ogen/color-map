{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts", "__test__/**/*.ts", "custom.d.ts"]
}
